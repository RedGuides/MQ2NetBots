			<Width>50</Width>
			<Heading> Dist</Heading>
		</Columns>
		<Columns>
			<Width>20</Width>
			<Heading></Heading>
		</Columns>
		<Columns>
			<Width>90</Width>
			<Heading>Name</Heading>
		</Columns>
		<Columns>
			<Width>120</Width>
			<Heading>Target</Heading>
		</Columns>
		<Columns>
			<Width>150</Width>
			<Heading>Action</Heading>
		</Columns>
	</Listbox>

	<Screen item = "NetBotsWnd">
		<RelativePosition>false</RelativePosition>
